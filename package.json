{
  "name": "pdf-linter",
  "version": "0.0.1",
  "main": "index.js",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "compile-textlint": "textlint-script-compiler --metadataName dev --metadataNamespace \"https://example.com\" --metadataHomepage \"https://example.com\" --mode development --output-dir public",
    "lint": "eslint --fix .",
    "lintcheck": "eslint .",
    "fmt": "prettier --no-semi --write \"**/*.{js,ts,tsx}\"",
    "fmtcheck": "prettier --no-semi --check \"**/*.{js,ts,tsx}\"",
    "predev": "npm run compile-textlint",
    "dev": "vite",
    "prebuild": "npm run compile-textlint",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@axe-core/react": "^4.4.3",
    "@emotion/css": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "pdfjs-dist": "~3.7.107",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "recoil": "^0.7.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^2.1.3",
    "@eslint/js": "^8.53.0",
    "@textlint/script-compiler": "^0.14.0",
    "@types/react": "^17.0.48",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.0",
    "textlint-rule-preset-ja-technical-writing": "^7.0.0",
    "textlint-rule-preset-japanese": "^7.0.0",
    "typescript": "^5.1.6",
    "vite": "^4.3.9"
  }
}
