{
  "name": "pdf-linter",
  "version": "0.0.1",
  "main": "index.js",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "compile-textlint": "textlint-script-compiler --metadataName dev --metadataNamespace \"https://example.com\" --metadataHomepage \"https://example.com\" --mode development --output-dir dist",
    "lint": "prettier --no-semi --write \"**/*.{js,ts,tsx}\"",
    "dev": "run-p dev:*",
    "dev:textlint": "npm run compile-textlint",
    "dev:webpack": "webpack-dev-server",
    "build": "run-s build:webpack build:textlint",
    "build:textlint": "npm run compile-textlint",
    "build:webpack": "webpack --mode production"
  },
  "devDependencies": {
    "@axe-core/react": "^4.4.3",
    "@textlint/script-compiler": "^0.12.1",
    "@types/pdfjs-dist": "^2.1.4",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "@types/react-redux": "^7.0.1",
    "copy-webpack-plugin": "^10.2.4",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "emotion": "^10.0.9",
    "html-webpack-plugin": "^5.5.0",
    "npm-run-all": "^4.1.5",
    "pdfjs-dist": "~2.4.456",
    "prettier": "^2.0.5",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "recoil": "^0.0.13",
    "textlint-rule-preset-ja-technical-writing": "^7.0.0",
    "textlint-rule-preset-japanese": "^7.0.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  }
}
