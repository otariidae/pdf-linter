{
  "name": "pdf-linter",
  "version": "0.0.1",
  "main": "index.js",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "dev:functions": "tsc -p tsconfig.functions.json --watch",
    "predev:hosting": "rm -rf dist/hosting",
    "dev:hosting": "webpack --watch",
    "dev:netlify": "cross-env NODE_ENV=development netlify dev",
    "dev": "run-p dev:*",
    "lint": "prettier --no-semi --write \"**/*.{js,ts,tsx}\"",
    "prebuild": "rm -rf dist",
    "build": "run-p build:*",
    "prebuild:hosting": "make-dir dist/hosting",
    "build:hosting": "run-p build:hosting:*",
    "build:hosting:cmaps": "cp -r ./node_modules/pdfjs-dist/cmaps dist/hosting/",
    "build:hosting:webpack": "webpack --mode production",
    "build:functions": "tsc -p tsconfig.functions.json"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.51",
    "@types/node": "^13.13.4",
    "@types/pdfjs-dist": "^2.1.4",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "@types/react-redux": "^7.0.1",
    "@types/recoil": "^0.0.0",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "emotion": "^10.0.9",
    "html-webpack-plugin": "^4.3.0",
    "make-dir-cli": "^2.0.0",
    "netlify-cli": "^2.48.0",
    "npm-run-all": "^4.1.5",
    "pdfjs-dist": "~2.3.200",
    "prettier": "^2.0.5",
    "react": "^16.8.0",
    "react-axe": "^3.1.0",
    "react-dom": "^16.8.0",
    "recoil": "^0.0.7",
    "ts-config-webpack-plugin": "^2.0.0",
    "ts-loader": "^7.0.2",
    "typescript": "^3.5.2",
    "web-streams-polyfill": "^2.1.1",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "textlint": "^11.0.1",
    "textlint-rule-preset-ja-technical-writing": "^4.0.0-1",
    "textlint-rule-preset-japanese": "^4.0.0"
  }
}
