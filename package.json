{
  "name": "pdf-linter",
  "version": "0.0.1",
  "main": "index.js",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist",
    "copy-cmaps": "cp -r ./node_modules/pdfjs-dist/cmaps dist/cmaps",
    "compile-textlint": "textlint-script-compiler --metadataName dev --metadataNamespace example.com --mode development --output-dir dist",
    "prepare-dir": "mkdir dist",
    "lint": "prettier --no-semi --write \"**/*.{js,ts,tsx}\"",
    "predev": "run-s clean prepare-dir",
    "dev": "run-p dev:*",
    "dev:cmaps": "npm run copy-cmaps",
    "dev:textlint": "npm run compile-textlint",
    "dev:webpack": "webpack-dev-server",
    "prebuild": "run-s clean prepare-dir",
    "build": "run-p build:*",
    "build:cmaps": "npm run copy-cmaps",
    "build:textlint": "npm run compile-textlint",
    "build:webpack": "webpack --mode production"
  },
  "devDependencies": {
    "@textlint/script-compiler": "^0.7.0",
    "@types/pdfjs-dist": "^2.1.4",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "@types/react-redux": "^7.0.1",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "emotion": "^10.0.9",
    "html-webpack-plugin": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "pdfjs-dist": "~2.3.200",
    "prettier": "^2.0.5",
    "react": "^16.8.0",
    "react-axe": "^3.1.0",
    "react-dom": "^16.8.0",
    "recoil": "^0.0.13",
    "textlint-rule-preset-ja-technical-writing": "^4.0.1",
    "textlint-rule-preset-japanese": "^5.0.0",
    "ts-config-webpack-plugin": "^2.0.0",
    "ts-loader": "^7.0.2",
    "typescript": "^3.5.2",
    "web-streams-polyfill": "^2.1.1",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {}
}
